{
  "name": "next-serverless",
  "version": "0.1.0",
  "description": "Runs your next.js app with serverless in the cloud",
  "main": "lib/index.js",
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "build": "babel src -d lib",
    "watch": "babel --watch src -d lib",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
